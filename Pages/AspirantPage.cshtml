@page
@model eStuff.Pages.AspirantPageModel
@{
}
<tbody>
    @foreach (var item in Model.Aspirants)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <br />
    </tr>
    }
</tbody>
